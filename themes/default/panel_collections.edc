		program {
			name: "cancel";
			signal: "mouse,clicked,1";
			source: "bt_cancel_event";
			action: SIGNAL_EMIT "coll_save_cancel" "coll_name_panel";
		}
		program {
			name: "save";
			signal: "mouse,clicked,1";
			source: "bt_save_event";
			action: SIGNAL_EMIT "cmd,coll,save" "gui";
			after: "cancel";
		}
	}
