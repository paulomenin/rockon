					size: 10;
					align: 0.5 0.5;
				}
			}
		}
		part {
			name: "bt_cancel_event";
			type: RECT;
			repeat_events: 1;
			description {
				state: "default" 0.0;
				color: 255 255 255 0;
				rel1{relative: 0.0 0.0; to:"bt_cancel";}
				rel2{relative: 1.0 1.0; to:"bt_cancel";}
			}
		}
		part {
			name: "bt_save";
			type: GROUP;
			source: "button";
			description {
				state: "default" 0.0;
				color: 255 0 0 50;
				rel1{relative: 1.0 0.0; to:"bt_cancel"; offset: 0 0;}
				rel2{relative: 1.0 1.0; to:"bg"; offset: -10 -5;}
			}
		}
		part {
			name: "bt_save_text";
			type: TEXT;
			effect: SOFT_SHADOW;
			mouse_events: 0;
			description {
				state: "default" 0.0;
				color_class: "font_title";
				rel1{relative: 0.0 0.0; to:"bt_save";}
				rel2{relative: 1.0 1.0; to:"bt_save";}
				text {
					text: "Save";
					font: "Sans Serif:style=Bold";
					size: 10;
					align: 0.5 0.5;
				}
			}
		}
		part {
			name: "bt_save_event";
			type: RECT;
			repeat_events: 1;
			description {
				state: "default" 0.0;
				color: 255 255 255 0;
				rel1{relative: 0.0 0.0; to:"bt_save";}
				rel2{relative: 1.0 1.0; to:"bt_save";}
			}
		}
	}
	programs {
		program {
			name: "cancel";
			signal: "mouse,clicked,1";
			source: "bt_cancel_event";
			action: SIGNAL_EMIT "pls_save_cancel" "pls_name_panel";
		}
		program {
			name: "save";
			signal: "mouse,clicked,1";
			source: "bt_save_event";
			action: SIGNAL_EMIT "cmd,pls,new" "gui";
			after: "cancel";
		}
	}
}

